# Zoom Team Chat Configuration
# Get these values from your Zoom App Marketplace dashboard at https://marketplace.zoom.us/
ZOOM_CLIENT_ID=your_zoom_client_id_here
ZOOM_CLIENT_SECRET=your_zoom_client_secret_here


ZOOM_BOT_JID=your_bot_jid_here
ZOOM_WEBHOOK_SECRET_TOKEN=your_verification_token_here
ROBOT_ZOOM_BOT_JID=https://zoom.us/launch/chat?jid=robot_your_bot_jid_here

ZOOM_REDIRECT_URI=https://your_tunnel_url_here/auth/callback

# Anthropic API Configuration
# Get your API key from https://console.anthropic.com/
ANTHROPIC_API_KEY=your_anthropic_api_key_here
ANTHROPIC_MODEL=claude-3-opus-20240229

# Frontend Configuration (optional)
# If you have a frontend application that needs CORS access
FRONTEND_ORIGIN=http://localhost:3000

ACCOUNT_ID=your_zoom_account_id_here

